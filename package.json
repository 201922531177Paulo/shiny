{
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "build": "grunt",
    "clean": "grunt clean",
    "watch": "grunt watch",
    "reactLog-staged": "grunt reactLog-staged",
    "lintjs": "./tools/lintjs_file.sh",
    "rlog-fix": "./tools/clean_rlog_file.sh",
    "rlog-fix-all": "npm run rlog-fix inst/www/rlog/src/**/*.js",
    "rlog-lint-all": "npm run lintjs inst/www/rlog/src/**/*.js",
    "rlog-build": "npm run rlog-lint-all && grunt webpack:reactLog",
    "rlog-build-only": "grunt webpack:reactLog",
    "rlog-watch": "npm run rlog-lint-all && (onchange 'inst/www/rlog/src/**/*.js' --verbose --wait -- npm run lintjs {{changed}} &) && trap 'kill $(jobs -p)' EXIT && grunt webpackReactLogWatch webpack:reactLog"
  },
  "lint-staged": {
    "inst/www/rlog/src/**/*.js": [
      "npm run rlog-fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-es2015-modules-umd": "6.24.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-flow": "6.23.0",
    "eslint": "^4.19.1",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-stylish-mapped": "^1.0.0",
    "grunt": "^1.0.2",
    "grunt-babel": "^6.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^18.0.0",
    "grunt-newer": "^1.1.2",
    "grunt-prettier": "0.5.1",
    "grunt-string-replace": "^1.3.1",
    "grunt-webpack": "^3.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.0",
    "onchange": "^4.0.0",
    "prettier": "1.12.1",
    "webpack": "^4.8.3"
  },
  "dependencies": {
    "cytoscape": "^3.2.12",
    "cytoscape-dagre": "^2.2.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10"
  }
}
